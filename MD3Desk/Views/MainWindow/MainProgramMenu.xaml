<UserControl x:Class="MD3Desk.Views.MainWindow.MainProgramMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:MD3Desk.Views.MainWindow"
             mc:Ignorable="d"
             d:DesignHeight="600" d:DesignWidth="800">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button Command="{Binding OpenStationEmulatorCommand}">Содержимое и сборка</Button>
        <Button Command="{Binding Parent.OpenStationEmulatorCommand}">Работа на эмуляторе</Button>
    </StackPanel>
</UserControl>
